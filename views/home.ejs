<%- include("partials/header.ejs") -%>

<h1>Home</h1>
<p><%= home %></p>

<% newPost.forEach(function(element){ %>
    <h1 style="display: block;"><%= element.title %></h1>
    <p style="display: block;"><%= element.post.substring(0,100) + " ..." %> <a href="/posts/<%= element.title %>">Read More</a> </p>
<% }); %>



